{
    "name": "Attentify",
    "version": "0.0.1",
    "description": "Focus on your studies!",
    "permissions": ["activeTab", "storage"],
    "host_permissions": [
      "<all_urls>"
    ],
    "action": {
      "default_popup": "popup/popup.html"
    },
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "css": ["notes.css"],
        "js": ["notes.js"]
      }
    ],
    "manifest_version": 3
  }
  